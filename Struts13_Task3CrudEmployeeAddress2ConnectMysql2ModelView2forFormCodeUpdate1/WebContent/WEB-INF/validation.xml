<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.3.0//EN"
    "http://commons.apache.org/dtds/validator_1_3_0.dtd">

<form-validation>

    <formset>

        <!-- Validation rules for RegistrationForm -->
        <form name="registrationForm">
            <field property="firstName" depends="required">
                <arg key="required" resource="false"/>
            </field>
            <field property="lastName" depends="required">
                <arg key="required" resource="false"/>
            </field>
            <!-- Add validation rules for other fields as needed -->
        </form>

    </formset>

</form-validation>
